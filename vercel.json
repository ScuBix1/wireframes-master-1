{
    "version": 2,
    "name": "tuto",
    "headers": [
      {
        "source": "/css/(.*)",
        "headers": [
          { "key": "Content-Type", "value": "text/css" }
        ]
      },
      {
        "source": "/js/(.*)",
        "headers": [
          { "key": "Content-Type", "value": "application/javascript" }
        ]
      }
    ],
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/server.js" }
    ]
}